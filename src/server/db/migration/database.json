{
  "development": {
    "driver": "pg",
    "user": {
      "ENV": "PGUSER"
    },
    "password": {
      "ENV": "PGPASSWORD"
    },
    "host": {
      "ENV": "PGHOST"
    },
    "database": {
      "ENV": "PGDATABASE"
    },
    "schema": {
      "ENV": "public"
    }
  },
  "production": {
    "ssl": {
      "rejectUnauthorized": false
    }
  }
}
