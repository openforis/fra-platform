{
  "name": "frap",
  "engines": {
    "node": "6.10.2"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:watch": "webpack --config webpack.config.js --watch",
    "clean": "rm -rf dist/",
    "start": "node server/server.js",
    "postinstall": "webpack --config webpack.config.js",
    "watch": "yarn build:watch & yarn watch:server & yarn livereload:app",
    "watch:server": "NODE_ENV=development nodemon -e js --watch server server/server.js",
    "livereload:app": "node --eval \"require('livereload').createServer().watch('./')\""
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "livereload": "^0.6.2",
    "nodemon": "^1.11.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-debounced": "^0.4.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
